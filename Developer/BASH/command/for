#循环输出1 2 3 4 5 6
for i in 1 2 3 4 5 6 
	do 
		echo $i
	done


#循环输出morning noon afternoon evening
for time in morning noon afternoon evening
	do
		echo "this is time is $time"
	done





for((初始值;循环变量;变量变化))
do
	程序
done
#从1加到100
#!/bin/bash

s=0
for((i=1;i<=100;i=i+1))
do
	s=$(( $s+$i ))
done
echo $s

