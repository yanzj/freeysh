# src Makefile

include ../Makefile.common

OBJS+=../startup/startup.o
OBJS+=main.o
OBJS+=stm32f10x_it.o

all: src

src: app.a

app.a: $(OBJS)
	$(AR) cr app.a $(OBJS)

.PHONY: src clean tshow
clean:
	rm -f app.a $(OBJS)
tshow:
	@echo "######################################################################################################"
	@echo "######## optimize settings: $(InfoTextLib), $(InfoTextSrc)"
	@echo "######################################################################################################"
